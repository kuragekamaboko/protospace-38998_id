 
<div>
<%= link_to  prototype_path(prototype.id), method: :get do %>
  <%= image_tag prototype.image, class: 'prototype-image'if prototype.image.attached? %>
  <h1><%= prototype.title %></h1>
  <p><%= prototype.catch_copy %></p>
  <% end %>
  <p><%= link_to prototype.user.name, '/users/show' %>
</div>

